      <endpoint address="http://soa.mop.gob.sv/mop/rh/Enlaces.svc" binding="basicHttpBinding" bindingConfiguration="BasicHttpBinding_IWcfEnlaces" contract="Mop.Rh.Enlaces.IWcfEnlaces" name="BasicHttpBinding_IWcfEnlaces"/>
      <endpoint address="http://soa.mop.gob.sv/mop/rh/Personal.svc" binding="basicHttpBinding" bindingConfiguration="BasicHttpBinding_IWcfPersonal" contract="Mop.Rh.Personal.IWcfPersonal" name="BasicHttpBinding_IWcfPersonal"/>
      <endpoint address="http://soa.mop.gob.sv/mop/audit/Huellas.svc" binding="basicHttpBinding" bindingConfiguration="BasicHttpBinding_IWcfHuellas" contract="Mop.Audit.Huellas.IWcfHuellas" name="BasicHttpBinding_IWcfHuellas"/>
      <endpoint address="http://soa.mop.gob.sv/mop/seg/Maestra.svc" binding="basicHttpBinding" bindingConfiguration="BasicHttpBinding_IWcfMaestra" contract="Mop.Seg.BLL.WcfMaestra.IWcfMaestra" name="BasicHttpBinding_IWcfMaestra"/>
      <endpoint address="http://vmt.sertracen.com.sv:8383/LicRpvWS/LicRpvWSService" binding="basicHttpBinding" bindingConfiguration="LicRpvWSPortBinding" contract="sertracen.LicRpvWS" name="LicRpvWSPort"/>
    </client>
  </system.serviceModel>
  <location path="errores"/>
  <location path="public/default.aspx">
    <system.web>
      <authorization>
        <allow users="*"/>
      </authorization>
      <globalization culture="es-SV" requestEncoding="utf-8" responseEncoding="utf-8"/>
    </system.web>
  </location>
